// Change the filter to your city
// Write the export function to export the results

var s2 = ee.ImageCollection("COPERNICUS/S2")
var urban = ee.FeatureCollection("users/ujavalgandhi/e2e/ne_10m_urban_areas")

var filtered = urban.filter(ee.Filter.eq('system:index', '00000000000000001205'))
var geometry = filtered.geometry()

var rgbVis = {
  min: 0.0,
  max: 3000,
  bands: ['B4', 'B3', 'B2'], 
};
var filtered = s2.filter(ee.Filter.lt('CLOUDY_PIXEL_PERCENTAGE', 30))
  .filter(ee.Filter.date('2019-01-01', '2020-01-01'))
  .filter(ee.Filter.bounds(geometry))

var image = filtered.median(); 

var clipped = image.clip(geometry)
Map.addLayer(clipped, rgbVis, 'Clipped') 

var exportImage = clipped.select('B.*')

Export.image.toDrive({
    image: exportImage,
    description: 'ALbany_Composite_Raw',
    folder: 'earthengine',
    fileNamePrefix: 'albany_composite_raw',
    region: geometry,
    scale: 20,
    maxPixels: 1e9
})

var visualized = clipped.visualize(rgbVis)
Map.addLayer(visualized, {}, 'Visualized Image') 

Export.image.toDrive({
    image: visualized,
    description: 'Albany_Composite_Visualized',
    folder: 'earthengine',
    fileNamePrefix: 'albany_composite_visualized',
    region: geometry,
    scale: 20,
    maxPixels: 1e9
})
